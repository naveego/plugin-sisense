{
  "id": "plugin-sisense",
  "displayName": "Sisense",
  "description": "Publisher which pulls data from a Sisense ElastiCube",
  "apiVersion": "2",
  "versionDescription": "Version 2",
  "version": "1.0.0",
  "iconFile": "icon.jpg",
  "executable": "Plugin-Sisense",
  "kind": "publisher",
  "os": "$GOOS",
  "arch": "$GOARCH",
  "canProduceMultipleSchemas": true,
  "canAcceptQueryBasedSchemas": false,
  "canUseOAuth": false,
  "canWrite": false,
  "configSchema": {
    "ui": {
      "ui:order": [
        "ServerName",
        "Username",
        "Password",
        "ElastiCube"
      ],
      "Password": {
        "ui:widget": "password"
      }
    },
    "schema": {
      "type": "object",
      "properties": {
        "ServerName" : {
          "type": "string",
          "title": "Server Name",
          "description": "The name of the Sisense server. (eg: LocalHost)"
        },
        "Username": {
          "type": "string",
          "title": "Username",
          "description": "The username to use for authentication"
        },
        "Password": {
          "type": "string",
          "title": "Password",
          "description": "The password to use for authentication"
        }, 
        "ElastiCube": {
          "type": "string",
          "title": "ElastiCube",
          "description": "The name of the Sisense ElastiCube to pull data from"
        }
      },
      "required": [
        "ServerName",
        "Username",
        "Password",
        "ElastiCube"
      ]
    }
  }
}